{
  "private": true,
  "name": "root",
  "license": "UNLICENSED",
  "version": "0.0.0",
  "scripts": {
    "start": "sapper dev",
    "ci:build": "run-s _svelte-check _sapper:export _copy:cname",
    "_copy:cname": "shx cp .CNAME __sapper__/export/CNAME",
    "_sapper:export": "sapper export --legacy",
    "_svelte-check": "svelte-check --ignore src/node_modules/@sapper"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "compression": "^1.7.4",
    "polka": "^0.5.2",
    "sirv": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@rollup/plugin-url": "^5.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/node": "^14.14.10",
    "@types/polka": "^0.5.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^1.4.1",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.10",
    "shx": "^0.3.3",
    "svelte": "^3.17.3",
    "svelte-check": "^1.1.17",
    "svelte-preprocess": "^4.6.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,svelte,css,json,md,yml}": [
      "prettier --write"
    ]
  }
}
